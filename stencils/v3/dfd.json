{
  "library": [
    {
      "name": "Data Flow Diagram",
      "id": "avabodh.dataflow",
      "category": "Data Flow Diagram",
      "group": "Software",
      "objects": [
        {
          "template": {
            "id": "avabodh.dataflow.exentity",
            "name": "Entity",
            "texts": [ { "location": "default", "text": "Entity Name" } ],
            "data": [
                "min(min(w, h)*0.1, 8)", "-w/2+d(0)", "-h/2+d(0)", "w/2", "-h/2+d(0)", "w/2", "h/2", "-w/2+d(0)", "h/2",
                "w/2-d(0)", "-h/2+d(0)", "w/2-d(0)", "-h/2", "-w/2", "-h/2", "-w/2", "h/2-d(0)", "-w/2+d(0)", "h/2-d(0)"
            ],
            "draw": [ 
                { "path": "M 1 2 L 3 4 L 5 6 L 7 8 Z" },
                { "path": "M 9 10 L 11 12 L 13 14 L 15 16 L 17 18 L 1 2 Z" }
            ],
            "style": {"fillpaint": { "type": 2, "color": -1 }},
            "libraryview": { "scale": true, "width": 100, "height": 70 }
          },
          "params": { "width": 130, "height": 100 }
        },
        {
          "template": {
            "id": "avabodh.dataflow.entity",
            "name": "Entity",
            "texts": [ { "location": "default", "text": "Entity Name" } ],
            "data": [ "-w/2", "-h/2", "w/2", "-h/2", "w/2", "h/2", "-w/2", "h/2" ],
            "draw": [ { "path": "M 0 1 L 2 3 L 4 5 L 6 7 Z" } ],
            "style": {"fillpaint": { "type": 2, "color": -1 } },
            "libraryview": { "scale": true, "width": 100, "height": 70 }
          },
          "params": { "width": 130, "height": 100 }
        },
        {
          "template": {
            "id": "avabodh.dataflow.process",
            "name": "Process",
            "texts": [ { "location": "default", "text": "1.0\nProcess" } ],
            "data": ["0", "0", "w", "h", "0"],
            "draw": [ { "path": "E 0 1 2 3 4" } ],
            "style": { "fillpaint": { "type": 2, "color": -1 } },
            "libraryview": { "scale": true, "width": 100, "height": 100 }
          },
          "params": { "width": 100, "height": 100 }
        },
        {
          "template": {
            "id": "avabodh.dataflow.rectprocess",
            "name": "Process",
            "data": [ 
                "-w/2", "-h/2", "w/2", "-h/2", "w/2", "h/2", "-w/2", "h/2",
                "max(min(h*6, 32), th(0)+8)",
                "-w/2", "-h/2+d(8)", "w/2", "-h/2+d(8)",
                "0", "-h/2+d(8)/2", "w", "d(8)", "0",
                "h-d(8)",
                "0", "h/2-d(18)/2", "w", "d(18)", "0"
            ],
            "draw": [
                { "path": "M 0 1 L 2 3 L 4 5 L 6 7 Z" } ,
                { "path": "M 9 10 L 11 12"} 
            ],
            "style": { "fillpaint": { "type": 2, "color": -1 } },
            "texts": [ 
                { "location": "13 14 15 16 17", "text": "1.0" },
                { "location": "19 20 21 22 23", "text": "Process" } 
            ],
            "libraryview": { "scale": true, "width": 100, "height": 70 }
          },
          "params": { "width": 130, "height": 100 }
        },
        {
          "template": {
            "id": "avabodh.dataflow.dsplines",
            "name": "Data Store",
            "data": [ 
                "-w/2", "-h/2", "w/2", "-h/2", "w/2", "h/2", "-w/2", "h/2"
            ],
            "draw": [
                {"path": "M 0 1 L 2 3 L 4 5 L 6 7 Z", "style": { 
                    "strokepaint": { "type": 1},
                    "nonoverridables": ["strokePaint"]
                    }
                } ,
                {"path": "M 0 1 L 2 3"},
                {"path": "M 4 5 L 6 7"} 
            ],
            "style": {"fillpaint": { "type": 2, "color": -1 }},
            "texts": [ { "location": "default", "text": "Data Store" } ],
            "libraryview": { "scale": true, "width": 100, "height": 70 }
          },
          "params": { "width": 150, "height": 75 }
        },
        {
          "template": {
            "id": "avabodh.dataflow.dstlines",
            "name": "Data Store",
            "data": [ 
                "w/2", "-h/2", "-w/2", "-h/2", "-w/2", "h/2", "w/2", "h/2"
            ],
            "draw": [
                {"path": "M 0 1 L 2 3 L 4 5 L 6 7 "}
            ],
            "style": { "fillpaint": { "type": 2, "color": -1 }},
            "texts": [ { "location": "default", "text": "Data Store" } ],
            "libraryview": { "scale": true, "width": 100, "height": 35 }
          },
          "params": { "width": 160, "height": 35 }
        },
        {
          "template": {
            "id": "avabodh.dataflow.dsflines",
            "name": "Data Store",
            "data": [ 
                "w/2", "-h/2", "-w/2", "-h/2", "-w/2", "h/2", "w/2", "h/2",
                "-w/2+w*0.2", "-h/2", "d(8)", "h/2",
                "w*0.1", "0", "w*0.8", "h", "0"
            ],
            "draw": [
                {"path": "M 0 1 L 2 3 L 4 5 L 6 7 "},
                {"path": "M 8 9 L 10 11 "}
            ],
            "style": { "fillpaint": { "type": 2, "color": -1 }},
            "texts": [ { "location": "12 13 14 15 16", "text": "Data Store" } ],
            "libraryview": { "scale": true, "width": 100, "height": 35 }
          },
          "params": { "width": 160, "height": 35 }
        }
    ]}
  ]
}
